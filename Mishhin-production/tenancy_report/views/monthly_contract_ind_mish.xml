<odoo>
    <data>
        <record id="paperformat_tenancy_report" model="report.paperformat">
            <field name="name">Tenancy Report Paper</field>
            <field name="default" eval="True"/>
            <field name="format"></field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">30</field>
            <field name="margin_bottom">23</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">30</field>
            <field name="dpi">90</field>
        </record>
        <template id="tenancy_report_header">
            <div class="header">
                <div style="padding-top:10px">
                    <img class="text-center" style="text-align: left;
                            margin: 0 auto;
                            display: block;
                            margin-left: auto;
                            margin-right: auto;" t-att-src="'/tenancy_report/static/src/img/logo_this.png'"/>
                </div>
            </div>
        </template>

        <template id="tenancy_report_footer">
            <div class="footer">
                <div style="padding-bottom:10px">
                    <img class="text-center" style="text-align: left;
                            margin: 0 auto;
                            display: block;
                            margin-left: auto;
                            margin-right: auto;" t-att-src="'/tenancy_report/static/src/img/footer.jpg'"/>
                </div>

            </div>
        </template>


        <!--        <template id="external_layout_header_m">-->
